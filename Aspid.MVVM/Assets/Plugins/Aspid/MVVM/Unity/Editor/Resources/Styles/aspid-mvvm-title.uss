VisualElement {}

> #TextContainer
{
    width: 100%;
    flex-direction: row;
    align-content: center;
    justify-content: space-between;
}

> #TextContainer > #Text
{
    color: #c0c0c0;
    font-size: 14;
    -unity-font-style: bold;
}

> #Line
{
    margin-top: 5px;
    margin-bottom: 5px;
    background-color: #c0c0c0;
    width: 100%;
    height: 2px;
}