VisualElement {}

> #TextContainer
{
    width: 100%;
    flex-direction: row;
    align-content: center;
    justify-content: space-between;
}

> #TextContainer > #Text
{
    font-size: 14;
    flex-shrink: 1;
    color: #c0c0c0;
    overflow: hidden;
    white-space: nowrap;
    -unity-font-style: bold;
    text-overflow: ellipsis;
}

> #Line
{
    width: 100%;
    height: 2px;
    margin-top: 5px;
    margin-bottom: 5px;
    background-color: #c0c0c0;
}